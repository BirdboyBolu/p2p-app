<BaseLabel>:


<MarkupLabel>:
    markup: True


<FlexMarkupLabel>:
    size_hint: None, None
    size: self.texture_size
    text_size: None, None


<HFlexMarkupLabel>:
    size_hint_x: None
    width: self.texture_size[0]
    text_size: None, None


<NormalLabel>:
    markup: True
	font_size: app.font_size_normal


<CustomIcon>:
    markup: True
    text: md_icon(self.icon)
